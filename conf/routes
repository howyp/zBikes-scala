# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /station/:id      controllers.Stations.view(id: String)
PUT     /station/:id      controllers.Stations.upsert(id: String)

DELETE  /station/all      controllers.Stations.removeAll

GET     /station/near/:lat/:long controllers.Stations.near(lat: Double, long: Double)